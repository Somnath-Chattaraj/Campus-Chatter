{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://chatterapi.campusify.site/api/:match*"
    },
    {
      "source": "/ws/:match*",
      "destination": "wss://realtime.campusify.site/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
